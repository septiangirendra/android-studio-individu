<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.septian.projectindividual">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectIndividual"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".TambahKelas"
            android:exported="false" />
        <activity
            android:name=".LihatDetailKelas"
            android:exported="false" />
        <activity
            android:name=".TambahPeserta"
            android:exported="false" />
        <activity
            android:name=".LihatDetailDataPeserta"
            android:exported="false" />
        <activity
            android:name=".TambahMateri"
            android:exported="false" />
        <activity
            android:name=".LihatDetailDataMateri"
            android:exported="false" />
        <activity
            android:name=".TambahInstruktur"
            android:exported="false" />
        <activity
            android:name=".LihatDetailDataActivityInstruktur"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>